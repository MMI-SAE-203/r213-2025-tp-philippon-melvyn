---
import layout from "../layouts/layout.astro";

const events = [
    {
        title: "Conférence sur les astres",
        date: "Lundi 12 Juillet",
        favori: true,
    },
    {
        title: "Atelier sur les étoiles",
        date: "Mardi 13 Juillet",
        favori: false,
    },
];
---

<layout>
    <h1>Agenda</h1>
    {
        events.map((event) => {
            <ul>
                <li>titre: {event.title}</li>
                <li>date: {event.date}</li>
            </ul>;
        })
    }
</layout>
